#include<stdio.h>
int main()
{
    int sum=0;
    for (int i=10;i<=100;i++)
    {
        int f=0;
        for(int j=2;j<i;j++)
        if(i%j==0)
        {
            f=1;
            break;
        }
        if(f!=1)
        sum+=i;
    }
    printf("The sum of prime nos between 10 to 100 is : %d",sum);
return 0;
}
